  <ItemGroup>
    <PackageReference Include="Azure.AI.OpenAI" Version="2.2.0-beta.1" />
    <PackageReference Include="Microsoft.Azure.SignalR" Version="1.27.0" />
  </ItemGroup>

</Project>
